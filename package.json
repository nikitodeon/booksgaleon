{
  "name": "booksgaleon",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "prisma generate",
    "jest": "jest --no-watch",
    "jest:watch": "jest --watch",
    "vitest": "vitest run",
    "vitest:watch": "vitest watch",
    "unit:watch": "concurrently \"npm run jest:watch\" \"npm run vitest:watch\"",
    "unit": "npm run jest && npm run vitest",
    "e2e:bag": "playwright test --config=playwright.bag.config.ts",
    "e2e:main": "playwright test --config=playwright.config.ts",
    "e2e": "npm run e2e:main && npm run e2e:bag",
    "e2e:ui": "playwright test --config=playwright.config.ts --ui && playwright test --config=playwright.bag.config.ts --ui",
    "e2e:ui:bag": "playwright test --config=playwright.bag.config.ts --ui",
    "e2e:ui:main": "playwright test --config=playwright.config.ts --ui",
    "e2e:debug": "playwright test --config=playwright.config.ts --debug && playwright test --config=playwright.bag.config.ts --debug",
    "e2e:debug:bag": "playwright test --config=playwright.bag.config.ts --debug",
    "e2e:debug:main": "playwright test --config=playwright.config.ts --debug",
    "test": "npm run unit && npm run e2e",
    "test:ui": "concurrently \"npm run unit:ui\" \"npm run e2e:ui\"",
    "test:all:debug": "concurrently \"npm run unit:debug\" \"npm run e2e:debug\""
  },
